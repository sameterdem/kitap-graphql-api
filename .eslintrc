{
    "extends": "airbnb-base",
    "plugins": ["import"],
    "rules": {
      "no-console": "off",
      "func-names": "off",
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allow": ["_id", "__v"]
        }
      ]
    }
  }
  